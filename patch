for f in $(git diff HEAD^ --name-only); do
	echo "patching: $f"
	cat > $f.temp << EOF
if (typeof exports === 'object' && typeof exports.nodeName !== 'string' && typeof define !== 'function') {
    var define = function (factory) {
        factory(require, exports, module);
    };
}
EOF
	cat $f >> $f.temp
	mv $f.temp $f
done
echo "complete"
