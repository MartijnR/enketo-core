<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:h="http://www.w3.org/1999/xhtml"
    xmlns:jr="http://openrosa.org/javarosa"
    xmlns:odk="http://www.opendatakit.org/xforms"
    xmlns:orx="http://openrosa.org/xforms"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:head>
        <h:title>School Survey Testing Form</h:title>
        <model odk:xforms-version="1.0.0">
            <itext>
                <translation default="true()" lang="English">
                    <text id="/data/repeat1:label">
                        <value>Repeat 1 - Building</value>
                    </text>
                    <text id="/data/repeat1/repeat11:label">
                        <value>Repeat 11 - Corridor</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111:label">
                        <value>Repeat 111 - Rooms</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/room_open_stat:label">
                        <value>Is the room open or able to open it?</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/room_open_stat/yes:label">
                        <value>Yes</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/room_open_stat/no:label">
                        <value>No</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype:label">
                        <value>121. Room Type</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype:hint">
                        <value>Please choose from the list below</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list1:label">
                        <value>Library</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list2:label">
                        <value>Teacher room</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list3:label">
                        <value>Adminstrative Office</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list4:label">
                        <value>Specialty room</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list5:label">
                        <value>Classroom</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list6:label">
                        <value>Computer lab</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list7:label">
                        <value>Science Lab</value>
                    </text>
                    <text id="/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list8:label">
                        <value>Other</value>
                    </text>
                </translation>
            </itext>
            <instance>
                <data id="test_school_form4">
                    <repeat1 jr:template="">
                        <repeat11 jr:template="">
                            <repeat111 jr:template="">
                                <room_open_stat/>
                                <school_roomtype/>
                            </repeat111>
                        </repeat11>
                    </repeat1>
                    <repeat1>
                        <repeat11>
                            <repeat111>
                                <room_open_stat/>
                                <school_roomtype/>
                            </repeat111>
                        </repeat11>
                    </repeat1>
                    <meta>
                        <instanceID/>
                    </meta>
                </data>
            </instance>
            <bind nodeset="/data/repeat1/repeat11/repeat111/room_open_stat" type="string"/>
            <bind nodeset="/data/repeat1/repeat11/repeat111/school_roomtype" relevant="selected( ../room_open_stat ,'yes')" type="string"/>
            <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
        </model>
    </h:head>
    <h:body>
        <group ref="/data/repeat1">
            <label ref="jr:itext('/data/repeat1:label')"/>
            <repeat nodeset="/data/repeat1">
                <group ref="/data/repeat1/repeat11">
                    <label ref="jr:itext('/data/repeat1/repeat11:label')"/>
                    <repeat nodeset="/data/repeat1/repeat11">
                        <group ref="/data/repeat1/repeat11/repeat111">
                            <label ref="jr:itext('/data/repeat1/repeat11/repeat111:label')"/>
                            <repeat nodeset="/data/repeat1/repeat11/repeat111">
                                <select1 ref="/data/repeat1/repeat11/repeat111/room_open_stat">
                                    <label ref="jr:itext('/data/repeat1/repeat11/repeat111/room_open_stat:label')"/>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/room_open_stat/yes:label')"/>
                                        <value>yes</value>
                                    </item>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/room_open_stat/no:label')"/>
                                        <value>no</value>
                                    </item>
                                </select1>
                                <select1 ref="/data/repeat1/repeat11/repeat111/school_roomtype">
                                    <label ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype:label')"/>
                                    <hint ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype:hint')"/>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list1:label')"/>
                                        <value>roomtype_list1</value>
                                    </item>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list2:label')"/>
                                        <value>roomtype_list2</value>
                                    </item>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list3:label')"/>
                                        <value>roomtype_list3</value>
                                    </item>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list4:label')"/>
                                        <value>roomtype_list4</value>
                                    </item>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list5:label')"/>
                                        <value>roomtype_list5</value>
                                    </item>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list6:label')"/>
                                        <value>roomtype_list6</value>
                                    </item>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list7:label')"/>
                                        <value>roomtype_list7</value>
                                    </item>
                                    <item>
                                        <label ref="jr:itext('/data/repeat1/repeat11/repeat111/school_roomtype/roomtype_list8:label')"/>
                                        <value>roomtype_list8</value>
                                    </item>
                                </select1>
                            </repeat>
                        </group>
                    </repeat>
                </group>
            </repeat>
        </group>
    </h:body>
</h:html>
